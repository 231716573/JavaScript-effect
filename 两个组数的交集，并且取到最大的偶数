<head>
	<meta charset="UTF-8">
	<title>document</title>

<script type="text/javascript">
var arr1 = [1,2,3,4,5,6];
var arr2 = [2,4,6,8,10];

var arr3 = new Array();

for(var i =0;i<arr1.length;i++){
    for(var j = 0;j<arr2.length;j++){
        if(arr1[i] == arr2[j] && arr1[i]%2 == 0){
            arr3.push(arr1[i])        
        }
    }
}
console.log(Math.max.apply(null, arr3))

</script>
