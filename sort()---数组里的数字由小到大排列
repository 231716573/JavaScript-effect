var arr = [1,5,8,10,15,26];

console.log(arr.sort());  //[1, 10, 15, 26, 5, 8]

function compare(num1, num2){
  var temp1 = parseInt(num1);
  var temp2 = parseInt(num2);
  
  if(temp1 < temp2){
    return -1;
  }else if(temp1 == temp2){
    return 0;
  }else{
    return 1;
  }
}

arr.sort(compare);   //函数名是对象的引用，所以只写名字就行。
console.log(arr.sort(compare));  //[1, 5, 8, 10, 15, 26]



